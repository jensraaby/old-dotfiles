brew install python
pip3 install neovim websocket-client sexpdata
pip2 install python2-neovim websocket-client sexpdata
npm install -g neovim

export DEIN_PATH=${HOME}/.local/share/dein
mkdir -p $DEIN_PATH
export DEIN_INSTALLER=`mktemp installer-XXX`
curl https://raw.githubusercontent.com/Shougo/dein.vim/master/bin/installer.sh > ${DEIN_INSTALLER}
sh ${DEIN_INSTALLER} ${DEIN_PATH}
rm ${DEIN_INSTALLER}

nvim +qall 
nvim +UpdateRemotePlugins +qall

